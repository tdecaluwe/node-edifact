{ "name": "edifact"
, "version": "1.0.1"
, "description": "JavaScript parser for UN/EDIFACT documents."
, "keywords": ["edifact", "edi"]
, "main": "index.js"
, "engines":
  { "node": ">=4.0"
  }
, "repository":
  { "type": "git"
  , "url": "https://github.com/tdecaluwe/node-edifact.git"
  }
, "license": "Apache-2.0"
, "author":
  { "name": "Tom De CaluwÃ©"
  , "email": "decaluwe.t@gmail.com"
  }
, "devDependencies":
  { "jasmine-es6": "^0.0.18"
  , "jsdoc-to-markdown": "^1.2.1"
  }
, "scripts":
  { "test": "node-modules/.bin/jasmine"
  , "template": "cat docs/*.md > template.md"
  , "docs": "npm run template && node_modules/.bin/jsdoc2md -d 3 -t template.md -f {parser,validator,tracker,counter,reader}.js > README.md && rm template.md"
  }
}
